<ul id="select_results" style="background-color:white; list-style-type: none;" >
  <li><strong>COMUNITIES</strong></li>
  <% communities.each do |com|%>
    <li value='<%= com.id %>'><%= link_to com.name, com %></li>
  <% end %>
  <li><strong>USERS</strong></li>
  <% users.each do |user|%>
    <li value='<%= user.id %>'><a href="/user/<%=user.name%>"><%=user.name%></a></li>
  <% end %>
</ul>